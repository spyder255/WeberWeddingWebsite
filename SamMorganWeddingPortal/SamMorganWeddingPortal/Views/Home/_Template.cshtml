@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="row theme-color-background gradient-75">
    <div class="col-xs-12">
        <div class="page-title-box-outer">
            <div class="page-title-box-inner">
                <div class="header-text theme-color-foreground">@ViewBag.Title</div>
            </div>
        </div>
    </div>
</div>

@RenderBody()

@section head
{
    @RenderSection("head", required: false)
}

@section scripts
{
    @RenderSection("scripts", required: false)
}
